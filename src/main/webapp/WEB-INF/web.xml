<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>abc</servlet-name>
		<servlet-class>com.khadri.jee.mvc.calc.controller.CalcServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>abc</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>yyy</servlet-name>
		<servlet-class>com.khadri.jee.mvc.calc.controller.CalcServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>yyy</servlet-name>
		<url-pattern>/sub</url-pattern>
	</servlet-mapping>


	<!-- The above resource mapping approach for one servlet may gives complexity 
		to identify across web.xml -->
</web-app>
